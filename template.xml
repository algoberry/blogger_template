<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE HTML>
<html lang="en-US">
	<head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
	<meta content="follow, index" name="robots" />
	<b:if cond='data:blog.pageType == "index"'>
	<title><data:blog.pageTitle/></title>
	<meta name="title" expr:content="data:blog.pageTitle" />
	<b:else/>
	<title><data:blog.pageName/></title>
	<meta name="title" expr:content="data:blog.pageName" />
	</b:if>
	<meta name="description" expr:content="data:view.description.escaped" />
	<meta property="og:locale" content="en_US" />
	<meta property="og:type" content="website" />
	<meta property="og:site_name" content="" /> <!-- Add Your Website Name -->
	<meta property="og:url" expr:content="data:blog.url" />
	<b:if cond='data:blog.pageType == "index"'>
	<meta property="og:title" expr:content="data:blog.pageTitle" />
	<b:else/>
	<meta property="og:title" expr:content="data:blog.pageName" />
	</b:if>
	<meta property="og:description" expr:content="data:view.description.escaped" />
	<meta property="og:image" content="https://lh3.googleusercontent.com/ULB6iBuCeTVvSjjjU1A-O8e9ZpVba6uvyhtiWRti_rBAs9yMYOFBujxriJRZ-A=w1200" />
	<meta name="twitter:card" content="summary" />
	<meta name="twitter:site" content="" /> <!-- Add Your Twitter Id -->
	<meta property="twitter:url" expr:content="data:blog.url" />
	<b:if cond='data:blog.pageType == "index"'>
	<meta property="twitter:title" expr:content="data:blog.pageTitle" />
	<b:else/>
	<meta property="twitter:title" expr:content="data:blog.pageName" />
	</b:if>
	<meta name="twitter:description" expr:content="data:view.description.escaped" />
	<meta name="twitter:image" content="https://lh3.googleusercontent.com/ULB6iBuCeTVvSjjjU1A-O8e9ZpVba6uvyhtiWRti_rBAs9yMYOFBujxriJRZ-A=w1200" />
	<link rel="preconnect" href="https://fonts.gstatic.com" />
	<link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@400;700&amp;display=swap" rel="stylesheet" />
	<b:skin><![CDATA[
	/* General Start */
	body {
	padding:0px;
	margin:0px;
	font-family:'Open Sans', sans-serif;
	color:#1b1c25;
	line-height:1.6;
	}
	
	h1, h2, h4, h3 {
	margin:0px;
	padding:0px;
	line-height:normal;
	}
	
	pre {
	margin:0px;
	margin-top:10px;
	margin-bottom:4px;
	}
	/* General End */

	/* Layout Start */
	.container {
	width:1048px;	
	margin-left:auto;
	margin-right:auto;
	}
	
	header {
	clear:both;
	margin-bottom:20px;
	border-bottom:1px solid #dadce0;
	padding-top: 10px;
	padding-bottom: 10px;
	font-size:15px;
	}
	
	section {
	width:748px;
	float:left;text-align:left;
	font-size:17px;
	}

	aside {
	width:300px;
	float:right;text-align:left;
	font-size:16px;
	}
	
	footer {
	clear:both;
	border-top: 1px solid #e8e8e8;
	margin-top:30px;
	padding-top:10px;
	padding-bottom:10px;
	font-size:14px;
	}
	/* Layout End */

	/* Ahref Start */
	a {
	text-decoration:none;
	color:#1184e8;
	}
	/* Ahref End */

	/* Header Start */
	header a {
	color:inherit;
	margin-right:15px;
	}
	/* Header End */
	
	/* Section Start */
	#showerror {
	margin: auto;
	font-size:40px;
	font-weight:bold;
	}
	
	#innersection {
	padding-right:20px;
	}
	
	time {
	color:#9e9d89;
	font-weight:bold;
	font-size:14px;
	}

	article {
	margin-bottom:40px;
	}

	article:last-child {
	margin-bottom:0px;
	}

	section h1, section h2 a {
	font-size:35px;
	}
	
	section h2 a {
	color:inherit;
	}
	
	section h2 a:focus, section h2 a:hover {
	background:#a1cae2;
	}
	
	section h3, section h4 {
	font-size:16px;
	}
	
	section h3 {
	margin-top:10px;
	}
	
	.outputdiv {
	font-size:16px;
	border-radius: 4px;
	padding:5px;
	color:#FFFFFF;
	background-color:#0a043c;
	}
	
	#download {
	margin-top:10px;
	margin-bottom:5px;
	}
	
	#download a {
	background-color: #4CAF50;
	color:white;
	text-align: center;
	font-size:14px;
	cursor:pointer;
	font-weight:bold;
	padding:3px;
	border-radius: 3px;
	}
	
	section ul {
	font-size:16px;
	}
	
	section ul, section ul li {
	list-style: none;
	margin: 0px !important;
	padding: 0px !important;
	}
	
	#relatedposts a {
	clear:both;
	}
	
	#post-comment {
	font-size:16px;
	}
	
	.post-end {
	margin-top:15px;
	margin-bottom:5px;
	padding: 10px 0;
	background: #fff url(https://1.bp.blogspot.com/-Oj3F66X8r5I/XiLMmrd_2uI/AAAAAAAAArY/rV6ZQ-FAOs8Lz6-Y9HODpA08raTYX5URgCLcBGAsYHQ/s320/border.png) repeat-x 0 top;
	}
	
	#post-comment a {
	color:inherit;
	font-weight:bold;
	font-size:14px;
	}
	
	#post-comment .user a {
	font-size:16px;
	}
	
	#post-comment .datetime a {
	color: #b8b0b0;
	font-size:13px;
	}
	
	.comments .comments-content .comment:first-child {
	padding-top:0px;
	}
	
	.comment-block {
	padding: 15px;
	border-left: 4px solid #5b8a72;
	border-top: 1px solid #bfcba8;
	border-right: 1px solid #bfcba8;
	border-bottom: 1px solid #bfcba8;	
	}
	
	#blog-pager {
	font-size:25px;
	font-weight:700;
	}
	/* Section End */

	/* Aside Start */
	aside h2 {
	font-size:16px;
	}
	
	aside .normalbox, aside .adsbox {
	margin-bottom:15px;
	}
	
	#PopularPosts1 ul, #PopularPosts1 ul li {
	list-style: none;
	margin: 0px !important;
	padding: 0px !important;
	}
	
	.label {
	background-color:#f4f4f2;
	color:inherit;
	text-align: center;
	font-size:15px;
	cursor:pointer;
	border-radius:3px;
	padding:4px;
	line-height:2.5;
	}
	
	/* Aside End */

	/* Code Start */
	code { 
	font-size:15px;
	}
	
	.filename {
	color:#21094e;
	}
	
	.codebox {
	color:#59D9D1;
	background-color:#000000;
	border-radius: 4px;
	border-left: 4px solid #605a56;
	padding: 1.1%;
	overflow: auto;
	}

	.linecolor {
	color:#FFFFFF;
	margin-right:10px;
	}

	.keywordcolor {
	color:#F5D67B;
	}

	.quotescolor {
	color:#FC7038;
	}

	.commentcolor {
	color:#b8b0b0;
	}
	
	.parenthesiscolor {
	color:#F5D67B;
	}

	.bracketcolor {
	color:#F5D67B;
	}
	/* Code End */

	/* SVG Start */
	.svg-icon-left-post {
	width: 20px;
	height: 20px;
	}

	.svg-icon-left-post path {
	fill: #FFFFFF;
	}

	.svg-icon-post {
	width: 20px;
	height: 20px;
	padding:5px;
	}

	.svg-icon-post path {
	fill: #FFFFFF;
	}
	/* SVG End */

	/* Social Media Start */
	.social-icon-bar {
	position: fixed;
	top: 50%;
	-webkit-transform: translateY(-50%);
	-ms-transform: translateY(-50%);
	transform: translateY(-50%);
	}

	.social-icon-bar a {
	display: block;
	text-align: center;
	padding: 14px;
	transition: all 0.3s ease;
	color: white;
	}

	.social-icon-bar a:hover {
	background-color: #000;
	}

	.facebook {
	background: #3B5998;
	}

	.twitter {
	background: #55ACEE;
	}

	.google {
	background: #dd4b39;
	}

	.linkedin {
	background: #007bb5;
	}

	.youtube {
	background: #bb0000;
	}

	.tumblr {
	background: #314358;
	}

	.pinterest {
	background: #bb0000;
	}

	.email {
	background: grey;
	}
	/* Social Media End */
	
	/* Mobile View Start */
	@media (max-width: 768px) {

		body {
		padding-left:5px;
		padding-right:5px;
		}
		
		.container {
		width:100%;
		}

		section {
		width:100%;
		float:none;
		}
		
		#innersection {
		padding-right:0px;
		}
		
		aside {
		float:none;
		}
		
		#blog-pager {
		font-size:20px;
		}
	}
	/* Mobile View End */
	
	]]></b:skin>
	</head>
	<body>	
		<b:section id="header-section" class="header-section" maxwidgets="1" showaddelement="no"></b:section>
		<b:section id="navigation-section" class="navigation-section" maxwidgets="1" showaddelement="no"></b:section>
		<header>
			<div class="container">
				<a expr:href="data:blog.homepageUrl">Home</a>
				<a expr:href='data:blog.homepageUrl + "search/label/Tools"'>Tools</a>
			</div>
		</header>
		<b:if cond='data:view.isError'>
		<b:else/>
			<b:if cond='data:blog.pageType == "index"'>
			<b:else/>
				<b:if cond='data:blog.isMobileRequest == "true"'>
				<b:else/>
					<div class="social-icon-bar">
					<a expr:href='"https://www.facebook.com/sharer/sharer.php?u=" + data:blog.url' target='_blank' rel='noopener' class='facebook' title='Share to Facebook'><svg class='svg-icon-left-post' viewBox='0 0 20 20'><path fill='none' d='M11.344,5.71c0-0.73,0.074-1.122,1.199-1.122h1.502V1.871h-2.404c-2.886,0-3.903,1.36-3.903,3.646v1.765h-1.8V10h1.8v8.128h3.601V10h2.403l0.32-2.718h-2.724L11.344,5.71z'></path></svg></a>
					<a expr:href='"https://twitter.com/intent/tweet?url=" + data:blog.url + "&amp;text=" + data:blog.pageName' target='_blank' rel='noopener' class='twitter' title='Share to Twitter'><svg class='svg-icon-left-post' viewBox='0 0 20 20'><path fill='none' d='M18.258,3.266c-0.693,0.405-1.46,0.698-2.277,0.857c-0.653-0.686-1.586-1.115-2.618-1.115c-1.98,0-3.586,1.581-3.586,3.53c0,0.276,0.031,0.545,0.092,0.805C6.888,7.195,4.245,5.79,2.476,3.654C2.167,4.176,1.99,4.781,1.99,5.429c0,1.224,0.633,2.305,1.596,2.938C2.999,8.349,2.445,8.19,1.961,7.925C1.96,7.94,1.96,7.954,1.96,7.97c0,1.71,1.237,3.138,2.877,3.462c-0.301,0.08-0.617,0.123-0.945,0.123c-0.23,0-0.456-0.021-0.674-0.062c0.456,1.402,1.781,2.422,3.35,2.451c-1.228,0.947-2.773,1.512-4.454,1.512c-0.291,0-0.575-0.016-0.855-0.049c1.588,1,3.473,1.586,5.498,1.586c6.598,0,10.205-5.379,10.205-10.045c0-0.153-0.003-0.305-0.01-0.456c0.7-0.499,1.308-1.12,1.789-1.827c-0.644,0.28-1.334,0.469-2.06,0.555C17.422,4.782,17.99,4.091,18.258,3.266'></path></svg></a>
					<a expr:href='"https://www.tumblr.com/share?v=3&amp;u=" + data:blog.url + "&amp;t=" + data:blog.pageName' target='_blank' rel='noopener' class='tumblr' title='Share to Tumblr'><svg class='svg-icon-left-post' viewBox='0 0 20 20'><path fill='none' d='M11.039,14.123c-0.231-0.129-0.414-0.303-0.498-0.518C10.53,13.568,10.54,9.17,10.54,9.17h3.253V6.75h-3.25V3.5L8.474,3.512C8.386,4.181,8.225,4.733,7.991,5.167C7.756,5.601,7.445,5.972,7.059,6.281C6.673,6.592,6.208,6.828,5.665,6.994l0.001,2.194L7.293,9.17v4.215c0,0.594,0.067,1.047,0.198,1.359c0.133,0.312,0.37,0.607,0.713,0.887c0.343,0.277,0.757,0.49,1.242,0.639c0.484,0.15,1.008,0.23,1.64,0.23c0.555,0,1.006-0.057,1.484-0.164c0.479-0.104,1.165-0.287,1.756-0.549l0.009-1.996c-0.692,0.428-1.549,0.59-2.247,0.59C11.693,14.381,11.344,14.293,11.039,14.123'></path></svg></a>
					</div>
				</b:if>
			</b:if>
		</b:if>
		<div class="container">
			<section>
				<div id="innersection">
					<b:if cond='data:view.isError'>
					<div id="showerror">Oops no result found :(</div>
					<b:else/>
						<b:section id="content-section" class="content-section" showaddelement="no">
						<b:widget id="Blog1" locked="false" title="Blog Posts" type="Blog">
						<b:includable id="main" var="top">
						<b:loop values="data:posts" var="post" index="loopindex">
							<script type='application/ld+json'>{
								"@context": "http://schema.org",
								"@type": "BlogPosting",
								"mainEntityOfPage": {
									"@type": "WebPage",
									"@id": "<data:post.url/>"
								},
								"headline": "<data:post.title/>","description": "<data:view.description.escaped/>","datePublished": "<data:post.timestampISO8601/>",
								"dateModified": "<data:post.lastUpdatedISO8601/>","image": {
								"@type": "ImageObject","url": "https://lh3.googleusercontent.com/ULB6iBuCeTVvSjjjU1A-O8e9ZpVba6uvyhtiWRti_rBAs9yMYOFBujxriJRZ-A=w1200",
								"height": 348,
								"width": 1200},"publisher": {
									"@type": "Organization",
									"name": "Blogger",
									"logo": {
										"@type": "ImageObject",
										"url": "https://lh3.googleusercontent.com/ULB6iBuCeTVvSjjjU1A-O8e9ZpVba6uvyhtiWRti_rBAs9yMYOFBujxriJRZ-A=h60",
										"width": 206,
										"height": 60
									}
								},"author": {
									"@type": "Person",
									"name": "<data:post.author/>"
								}
							}</script>
							<article> 
								<time><data:post.timestamp/></time>
								<b:if cond='data:blog.pageType == "index"'>
								<h2><a expr:href="data:post.url" rel="follow"><data:post.title/></a></h2>
								<b:else/>
								<h1><data:post.title/></h1>
								</b:if>
								<b:if cond='data:blog.pageType != "index"'>
									<b:if cond='data:blog.isMobileRequest == "true"'>
										<div>
											<a expr:href='"https://www.facebook.com/sharer/sharer.php?u=" + data:blog.url' target='_blank' rel='noopener'><svg class='svg-icon-post facebook' viewBox='0 0 20 20'><path fill='none' d='M18.258,3.266c-0.693,0.405-1.46,0.698-2.277,0.857c-0.653-0.686-1.586-1.115-2.618-1.115c-1.98,0-3.586,1.581-3.586,3.53c0,0.276,0.031,0.545,0.092,0.805C6.888,7.195,4.245,5.79,2.476,3.654C2.167,4.176,1.99,4.781,1.99,5.429c0,1.224,0.633,2.305,1.596,2.938C2.999,8.349,2.445,8.19,1.961,7.925C1.96,7.94,1.96,7.954,1.96,7.97c0,1.71,1.237,3.138,2.877,3.462c-0.301,0.08-0.617,0.123-0.945,0.123c-0.23,0-0.456-0.021-0.674-0.062c0.456,1.402,1.781,2.422,3.35,2.451c-1.228,0.947-2.773,1.512-4.454,1.512c-0.291,0-0.575-0.016-0.855-0.049c1.588,1,3.473,1.586,5.498,1.586c6.598,0,10.205-5.379,10.205-10.045c0-0.153-0.003-0.305-0.01-0.456c0.7-0.499,1.308-1.12,1.789-1.827c-0.644,0.28-1.334,0.469-2.06,0.555C17.422,4.782,17.99,4.091,18.258,3.266'></path></svg></a>
											<a expr:href='"https://twitter.com/intent/tweet?url=" + data:blog.url + "&amp;text=" + data:blog.pageName' target='_blank' rel='noopener'><svg class='svg-icon-post twitter' viewBox='0 0 20 20'><path fill='none' d='M11.344,5.71c0-0.73,0.074-1.122,1.199-1.122h1.502V1.871h-2.404c-2.886,0-3.903,1.36-3.903,3.646v1.765h-1.8V10h1.8v8.128h3.601V10h2.403l0.32-2.718h-2.724L11.344,5.71z'></path></svg></a>
											<a expr:href='"https://www.tumblr.com/share?v=3&amp;u=" + data:blog.url + "&amp;t=" + data:blog.pageName' target='_blank' rel='noopener'><svg class='svg-icon-post tumblr' viewBox='0 0 20 20'><path fill='none' d='M11.039,14.123c-0.231-0.129-0.414-0.303-0.498-0.518C10.53,13.568,10.54,9.17,10.54,9.17h3.253V6.75h-3.25V3.5L8.474,3.512C8.386,4.181,8.225,4.733,7.991,5.167C7.756,5.601,7.445,5.972,7.059,6.281C6.673,6.592,6.208,6.828,5.665,6.994l0.001,2.194L7.293,9.17v4.215c0,0.594,0.067,1.047,0.198,1.359c0.133,0.312,0.37,0.607,0.713,0.887c0.343,0.277,0.757,0.49,1.242,0.639c0.484,0.15,1.008,0.23,1.64,0.23c0.555,0,1.006-0.057,1.484-0.164c0.479-0.104,1.165-0.287,1.756-0.549l0.009-1.996c-0.692,0.428-1.549,0.59-2.247,0.59C11.693,14.381,11.344,14.293,11.039,14.123'></path></svg></a>
										</div>
									</b:if>
								</b:if>
								<data:post.body/>
								<b:if cond='data:blog.pageType != "index"'>
									<div class="post-end"></div>
									<div id="post-comment">
									<b:include cond='data:blog.pageType in {"static_page","item"}' data='post' name='comment_picker'/>	
									</div>
								</b:if>
							</article>
						</b:loop>
						<div id="blog-pager">
							<b:if cond='data:newerPageUrl'>
							<span id="blog-pager-newer-link">								
							<a class="blog-pager-newer-link" expr:href="data:newerPageUrl" title="Newer Post" rel="follow">Newer Post</a>
							</span>
							</b:if>
							<b:if cond='data:olderPageUrl'>
							<span id="blog-pager-older-link">
							<a class="blog-pager-older-link" expr:href="data:olderPageUrl" title="Older Post" rel="follow">Older Post</a>
							</span>
							</b:if>
							<a class="home-link" expr:href="data:blog.homepageUrl" rel="follow">Home</a>
						</div>
						</b:includable>
						</b:widget>
						</b:section>
					</b:if>
				</div>
			</section>
			<aside>		
				<div class="normalbox">
				<h2>Popular Posts</h2>
				<b:section class='section' id='customsection1' showaddelement='no'>
					<b:widget id='PopularPosts1' locked='false' type='PopularPosts'>
						<b:includable id='main'>
							<ul>
							<b:loop values='data:posts' var='post'>
							<li><a expr:href='data:post.href'><data:post.title/></a></li>
							</b:loop>
							</ul>
						</b:includable>
					</b:widget>
				</b:section>
				</div>
				<div class="normalbox">
				<h2>Label</h2>
				<b:section class='section' id='customsection2' showaddelement='no'>
					<b:widget id='Label1' locked='false' type='Label'>
						<b:includable id='main'>
							<b:loop values='data:labels' var='label'>
							<a expr:dir='data:blog.languageDirection' expr:href='data:label.url' class="label"><data:label.name/></a>
							</b:loop>
						</b:includable>
					</b:widget>
				</b:section>
				</div>
				<div class="adsbox">
				</div>
				<div class="adsbox">
				</div>
			</aside>
		</div>
		<div class="container">
			<footer>
			Copyright &#9400; (Add Your Domain Name Here) | Powered by <a href="https://www.blogger.com">Blogger.com</a>
			</footer>
		</div>
		<b:section id="pull-section" class="pull-section"></b:section>
	</body>
</html>