<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html>
<html lang="en-US">
<head>
	<meta charset="UTF-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
	<meta content="follow, index" name="robots" />
	<b:if cond='data:blog.pageType == "index"'>
	<title><data:blog.pageTitle/></title>
	<meta name="title" expr:content="data:blog.pageTitle" />
	<b:else/>
	<title><data:blog.pageName/></title>
	<meta name="title" expr:content="data:blog.pageName" />
	</b:if>
	<meta name="description" expr:content="data:view.description.escaped" />
	<meta property="og:locale" content="en_US" />
	<meta property="og:type" content="website" />
	<meta property="og:site_name" expr:content="data:blog.pageTitle" />
	<meta property="og:url" expr:content="data:blog.url" />
	<b:if cond='data:blog.pageType == "index"'>
	<meta property="og:title" expr:content="data:blog.pageTitle" />
	<b:else/>
	<meta property="og:title" expr:content="data:blog.pageName" />
	</b:if>
	<meta property="og:description" expr:content="data:view.description.escaped" />
	<meta property="og:image" content="https://lh3.googleusercontent.com/ULB6iBuCeTVvSjjjU1A-O8e9ZpVba6uvyhtiWRti_rBAs9yMYOFBujxriJRZ-A=w1200" />
	<meta name="twitter:card" content="summary" />
	<meta name="twitter:site" content="" /> <!-- Add Your Twitter Id -->
	<meta property="twitter:url" expr:content="data:blog.url" />
	<b:if cond='data:blog.pageType == "index"'>
	<meta property="twitter:title" expr:content="data:blog.pageTitle" />
	<b:else/>
	<meta property="twitter:title" expr:content="data:blog.pageName" />
	</b:if>
	<meta name="twitter:description" expr:content="data:view.description.escaped" />
	<meta name="twitter:image" content="https://lh3.googleusercontent.com/ULB6iBuCeTVvSjjjU1A-O8e9ZpVba6uvyhtiWRti_rBAs9yMYOFBujxriJRZ-A=w1200" />
	<link href="https://fonts.googleapis.com/css2?family=Merriweather:wght@300" rel="stylesheet" />
	<b:skin><![CDATA[

	/* Body Start */
	body {
	font-family: 'Merriweather', serif;
	line-height: 1.6875em;
	word-spacing: 1px;
	padding:0px;
	margin:0px;
	font-size: 14px;
	color:#1C1C1C;
	}
	/* Body End */	
	
	/* Layout Start */
	header {
	width:100%;
	margin-bottom:15px;
	background-color:#f7f6e7;
	font-size: 14px;
	font-weight:bolder;
	/*
	border-bottom: 1px solid gray;
	*/
	/*
	box-shadow: 0 0 7px rgba(0, 0, 0, 0.1) !important;
	-moz-box-shadow: 0 0 7px rgba(0, 0, 0, 0.1) !important;
	-webkit-box-shadow: 0 0 7px rgba(0, 0, 0, 0.1) !important;
	*/
	}
	
	nav {
	
	}

	.container {
	width:1058px; /* 728px 30px 300px */
	margin-left: auto;
	margin-right: auto;
	}

	section {
	width:758px;
	float: left;text-align: left;
	height: auto;
	font-size: 16px;
	line-height: 1.6875em;
	word-spacing: 1px;
	}

	.innersection {
	padding:0px;
	padding-right:30px;
	}

	aside {
	width:300px;
	float: right;text-align: left;
	height: auto;
	font-size: 14px;
	}

	.inneraside {
	}

	.inneraside .colorbox {
	background-color:#e7e6e1;
	margin-bottom:5px;
	}

	.inneraside .normalbox {
	margin-bottom:20px;
	}
	
	.innersidediv .adsbox {
	margin-bottom:2px;
	}

	footer {
	border-top: 2px solid #000000;
	margin-top:60px;
	margin-bottom:15px;
	}
	
	.innerfooter {
	margin-top:17px;
	}
	/* Layout End */
	
	/* Href Link Start */
	a, a:active, a:focus {
	outline: none; 
	}

	a {
	text-decoration:none;
	}

	a:link {
	color: #111f4d;
	}

	a:visited {
	color: #111f4d;
	}

	a:hover {
	color: #111f4d;
	}

	a:active {
	color: #111f4d;
	}
	/* Href Link End */
	
	/* Menu Start */
	.menu {
	padding:0px;
	padding-top:8px;
	padding-bottom:8px;
	}
	
	.menulink {
	padding-right:20px;
	}

	menulink a {
	color:#f7f6e7;
	}
	/* Menu End */
	
	/* Mobile View Start */
	@media (max-width: 768px) {

	.menu {
	padding-left:5px;
	padding-right:5px;
	}

	.container {
	width:100%;
	}

	section {
	width:100%;
	float: none;
	}

	.innersection {
	padding:0px;
	padding-left:5px;
	padding-right:5px;
	}

	aside {
	width:100%;
	float: none;
	}

	.inneraside {
	margin-left:5px;
	margin-right:5px;
	margin-bottom:10px;
	}
	
	footer {
	margin-left:5px;
	margin-right:5px;
	}
	
	}
	/* Mobile View End */
	
	/* section Start */
	.post-published {
	color: #b8b0b0;
	font-size:14px;
	}
	
	section h1, section h2 {
	font-size:26px;
	font-weight:bolder;
	margin-top:5px;
	margin-bottom:10px;
	color: #111f4d;
	line-height: 1.2975em;
	word-spacing: 1px;
	}
	/*
	.post-title h2 a:focus, .post-title h2 a:hover {
	background: #99e1e5;
	}
	*/
	article h2 a:focus, article h2 a:hover {
	background: #99e1e5;
	}
	
	.postads {
	margin-bottom:20px;
	}
	
	article {
	margin-bottom:40px;
	}
	
	.blog-pager {
	font-weight:bolder;
	font-size:14px;
	}
	
	section h4 {
	font-size:13px;
	margin-top:5px;
	margin-bottom:10px;
	}
	
	.downloadlink {
	font-size:14px;
	font-weight:bolder;
	margin-top:4px;
	margin-bottom:8px;
	}
		
	.post-end {
	margin-top:15px;
	margin-bottom:5px;
	padding: 10px 0;
	/* Download the image first, upload any blog post and update the new image url */
	background: #fff url(https://1.bp.blogspot.com/-Oj3F66X8r5I/XiLMmrd_2uI/AAAAAAAAArY/rV6ZQ-FAOs8Lz6-Y9HODpA08raTYX5URgCLcBGAsYHQ/s320/border.png) repeat-x 0 top;
	}
	
	.iserror {
	text-align: center;
	font-size:19px;
	margin-top:60px;
	}
	
	.highlight {
	background-color:#00818a;
	}
	
	.status-msg-wrap {
	font-size: 14px;
	}
	
	.relatedposts {
	margin-top:20px;
	}
	
	.relatedposts h4 {
	margin-top:0px;
	margin-bottom:0px;
	}
	
	.relatedposts a {
	font-size:14px;
	}
	
	pre {
	margin-top:5px;
	margin-bottom:8px;
	}
	
	/* section End */
	
	
	/* aside Start */
	aside h2 {
	font-size:14px;
	font-weight:bolder;
	margin-top:0px;
	margin-bottom:0px;
	}
	
	.FollowByEmail .follow-by-email-inner .follow-by-email-address {
	border: 1px solid #cccccc;
	}
	
	.FollowByEmail .follow-by-email-inner .follow-by-email-submit {
	background: #111f4d;
	}
		
	.popular-posts ul {
	padding:0px;
	margin: 0 !important;
    padding: 1px !important;
	}

	.popular-posts ul li {
	list-style: none;
	margin: 0 !important;
    padding: 1px !important;
    line-height: 1.6800em;
	}
	/* aside End */
	
	/* code Section Start */
	.codebox {
	border-radius: 4px;
	-webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .05);
	box-shadow: inset 0 1px 1px rgba(0, 0, 0, .05);
	line-height: 1.2000em;
	padding: 1.1%;
	font-size: 16px;
	overflow: auto;
	color:#59D9D1;
	background-color:#001628;
	border: 1px solid #e3e3e3;
	border-left: 4px solid #605a56;
	}

	.lin {
	color:#FFFFFF;
	}

	.fN {
	color:#286090;
	margin-top:10px;
	}

	.kC {
	color:#F5D67B;
	}

	.pC {
	color:#F5D67B;
	}

	.iPC {
	color:#F5D67B;
	}

	.bC {
	color:#F5D67B;
	}

	.iBC {
	color:#F5D67B;
	}

	.qC {
	color:#FC7038;
	}

	.cC {
	color:#b8b0b0;
	}
	/* code Section End */
	
	/* SVG Section Start */
	.svg-icon-left-post {
	width: 20px;
	height: 20px;
	}

	.svg-icon-left-post path {
	fill: #FFFFFF;
	}
	
	.svg-icon-connect {
	width: 20px;
	height: 20px;
	padding:5px;
	background-color:#111f4d;
	}

	.svg-icon-connect path {
	fill: #FFFFFF;
	}
	
	.svg-icon-download {
	width: 20px;
	height: 20px;
	padding:5px;
	background-color:#111f4d;
	}

	.svg-icon-download path {
	fill: #FFFFFF;
	}
	
	.svg-icon-post {
	width: 20px;
	height: 20px;
	padding:5px;
	}

	.svg-icon-post path {
	fill: #FFFFFF;
	}
	/* SVG Section End */
	
	/* Social Media Start */
	.social-icon-bar {
	position: fixed;
	top: 50%;
	-webkit-transform: translateY(-50%);
	-ms-transform: translateY(-50%);
	transform: translateY(-50%);
	}

	.social-icon-bar a {
	display: block;
	text-align: center;
	padding: 14px;
	transition: all 0.3s ease;
	color: white;
	font-size: 20px;
	}

	.social-icon-bar a:hover {
	background-color: #000;
	}

	.facebook {
	background: #3B5998;
	}

	.twitter {
	background: #55ACEE;
	}

	.google {
	background: #dd4b39;
	}

	.linkedin {
	background: #007bb5;
	}

	.youtube {
	background: #bb0000;
	}

	.tumblr {
	background: #314358;
	}

	.pinterest {
	background: #bb0000;
	}

	.email {
	background: grey;
	}
	/* Social Media End */
	
	/* Comment Section Start */
	.comments .comment-block {
	padding: 15px;
	margin-left: 60px;
	border-left: 3px solid #ddd;
	border-top: 3px solid #48B1D9;
	border-right: 1px solid #DDD;
	border-bottom: 1px solid #DDD;
	}
	
	.comments .comments-content .comment-header, .comments .comments-content .comment-content {
	margin: 0 0 1px;
	}
	
	.user a {
	font-size:13px;
	color:#000000;
	}

	.comment-reply a {
	font-size:13px;
	color:#000000;
	}
	
	.datetime a {
	font-size:13px;
	color:#b8b0b0;
	margin-left:0px !important;
	}
	
	.blog-author {
	width:0px;
	}
	
	.comment-content {
	font-size:13px;
	}
	
	.comment-actions  a {
	font-size:12px;
	color:#000000;
	font-weight:bolder;
	}

	.thread-count a {
	font-size:13px;
	color:#000000;
	font-weight:bold;
	}

	.continue a {
	font-size:13px;
	color:#000000;
	}
	/* Comment Section End */
	
	]]></b:skin>
</head>
