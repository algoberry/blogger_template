				</div>
			</b:if>
		</b:if>
	</b:if>
	<div class="container">
		<section>
			<div class="innersection">				
				<b:if cond='data:view.isError'>
				<div class="iserror">
				Oops no result found :(
				</div>
				<b:else/>
					<b:section id="content-section" class="content-section" showaddelement="no">
						<b:widget id="Blog1" locked="false" title="Blog Posts" type="Blog">
						<b:includable id="main" var="top">
						<b:include data="top" name="status-message"/>
						<div>
						<b:loop values="data:posts" var="post" index="loopindex">
							<b:if cond='data:loopindex == 3'>
								<div class="postads">
								</div>
							</b:if>
							<article>
								<script type='application/ld+json'>{
								"@context": "http://schema.org",
								"@type": "BlogPosting",
								"mainEntityOfPage": {
									"@type": "WebPage",
									"@id": "<data:post.url/>"
								},
								"headline": "<data:post.title/>","description": "<data:view.description.escaped/>","datePublished": "<data:post.timestampISO8601/>",
								"dateModified": "<data:post.lastUpdatedISO8601/>","image": {
								"@type": "ImageObject","url": "https://lh3.googleusercontent.com/ULB6iBuCeTVvSjjjU1A-O8e9ZpVba6uvyhtiWRti_rBAs9yMYOFBujxriJRZ-A=w1200",
								"height": 348,
								"width": 1200},"publisher": {
									"@type": "Organization",
									"name": "Blogger",
									"logo": {
										"@type": "ImageObject",
										"url": "https://lh3.googleusercontent.com/ULB6iBuCeTVvSjjjU1A-O8e9ZpVba6uvyhtiWRti_rBAs9yMYOFBujxriJRZ-A=h60",
										"width": 206,
										"height": 60
									}
								},"author": {
									"@type": "Person",
									"name": "<data:post.author/>"
								}
								}</script>
								<div class="post-published"><data:post.timestamp/></div>							
								<b:if cond='data:blog.pageType == "index"'>
								<h2><a expr:href="data:post.url" rel="follow"><data:post.title/></a></h2>
								<!--<div class="post-title"><h2><a expr:href="data:post.url" rel="follow"><data:post.title/></a></h2></div>-->
								<b:else/>
									<h1><data:post.title/></h1>
									<!--<div class="post-title"><h1><data:post.title/></h1></div>-->
									<b:if cond='data:blog.pageType != "index"'>
