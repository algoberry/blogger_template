	<b:section id="navigation-section" class="navigation-section" maxwidgets="1" showaddelement="no"></b:section>
	<header>		
		<nav>
			<div class="container">
				<div class="menu">
				<a expr:href="data:blog.homepageUrl" class="menulink">Home</a>
				</div>
			</div>
		</nav>	 
	</header>
	<b:if cond='data:view.isError'>
	<b:else/>
		<b:if cond='data:blog.pageType == "index"'>
